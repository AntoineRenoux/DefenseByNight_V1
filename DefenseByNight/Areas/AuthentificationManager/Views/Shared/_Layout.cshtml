<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Translate.GetTraduction(EnumRessource.GEN_LBL_SITE_NAME).Text - @ViewBag.Title </title>
    @Styles.Render("~/Content/fontawesome")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/customLogin")
    @RenderSection("head", required: false)
</head>
<body class="getBackgroundImage">
    <div class="center">
        @RenderBody()
    </div>
</body>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/jqueryval")
@Scripts.Render("~/bundles/moment")
@Scripts.Render("~/bundles/unbostrusive-ajax")
@Scripts.Render("~/bundles/bootstrap")

@Scripts.Render("~/bundles/custom-validator-auth")

@RenderSection("scripts", required: false)
</html>
